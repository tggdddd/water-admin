<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.demo.water.mapper.WaterShopModelMapper">
    <delete id="deleteByMainId">
        DELETE
        FROM water_shop_model
        where shop_id = #{mainId}
    </delete>
    <select id="selectByMainId" resultType="org.jeecg.modules.demo.water.entity.WaterShopModel">
        select *
        from water_shop_model
        where shop_id = #{mainId}
    </select>
</mapper>